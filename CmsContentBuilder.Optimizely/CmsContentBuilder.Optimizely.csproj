<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Version>1.0.21-alpha</Version>
    <Title>CmsContentBuilder for Optimizely CMS</Title>
    <Company>MDR Solutions</Company>
    <Authors>Milos Rankovic</Authors>
    <Description>Unit testing helper for Optimizely CMS</Description>
    <RepositoryUrl>https://github.com/milosranko/CmsContentBuilder</RepositoryUrl>
    <Copyright>MDR Solutions DOO</Copyright>
    <PackageProjectUrl>https://github.com/milosranko/CmsContentBuilder</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="EPiServer.CMS" Version="12.16.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\CmsContentBuilder.Shared.Resources\CmsContentBuilder.Shared.Resources.csproj">
      <Private>True</Private>
      <CopyLocalSatelliteAssemblies>True</CopyLocalSatelliteAssemblies>
    </ProjectReference>
  </ItemGroup>

</Project>
